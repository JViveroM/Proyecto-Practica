package view;

import modelo.matriz;

public class vista {
	
	int [][] matrix;
	matriz m ;
	
	public vista() {
		matrix = new int [4][4];
	}
	public void imprimirMatriz() {
		for (int x = 0; x < matrix.length; x++) {
			  for (int y = 0; y < matrix[x].length; y++) {
			    System.out.print (m.getMatrix());
			    if (y != matrix[x].length-1) {
			    	System.out.print("\t");
			    }
			  }
			}
	}
}
